void summ(int ind, int sum, vector<int> arr, int n, vector<int> &vec){
    if(ind==n){
        vec.push_back(sum);
        return;
    }
    summ(ind+1,sum+arr[ind],arr,n,vec);
    summ(ind+1,sum,arr,n,vec);
}

vector<int> subsetSums(vector<int> arr, int N)
{
    vector<int> vec;
    summ(0,0,arr,N,vec);

    return vec;
}
